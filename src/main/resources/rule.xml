<rule>
	<tableRule name="rule1">
		<columns>TRANS_ID</columns>
		<dbRuleList>
			<dbRule><![CDATA[#set($start=$TRANS_ID.length() - 2)##
			#set($end=$TRANS_ID.length() - 1)##
			$TRANS_ID.substring($start,$end)]]>
			</dbRule>
		</dbRuleList>
		<tbRuleList>
			<tbRule><![CDATA[#set($start=$TRANS_ID.length() - 2)##
			 $TRANS_ID.substring($start)]]></tbRule>
		</tbRuleList>
		<!-- 00-99 100个表,每个表属于哪个结点 Map<Integer,Set<String>> -->
		<tbPrefix>
				<![CDATA[ 	
			            def map = [:];
						for (int i=0; i<10; i++) {
						   def list = [];
							for (int j=0; j<10; j++) {
								list.add(i+""+j);
							}
							 map.put(i,list);
						};
						return map; 
				]]>
		</tbPrefix>
	</tableRule>
</rule>
